# Confluence API Configuration
# Copy this file to .env and fill in your actual values

# Confluence instance base URL (required)
# Example: https://confluence.example.com
CONFLUENCE_BASE_URL=

# Confluence username (required)
CONFLUENCE_USERNAME=

# Confluence API token (recommended)
# Generate from: https://id.atlassian.com/manage-profile/security/api-tokens
CONFLUENCE_API_TOKEN=

# Request timeout in milliseconds (optional, default: 30000)
CONFLUENCE_TIMEOUT=30000
